{
    "manifest_version": 2,
    "name": "Acronym Translator",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["jquery-3.4.1.min.js","content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}